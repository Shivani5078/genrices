package company;

import java.util.Scanner;

public class FindPowerOfTwo {
	public static void main(String[] args) {
		//decleration part
		int c;
		Scanner sc = new Scanner(System.in);
		//user input
		System.out.println("Enter the number");
		c = sc.nextInt();
		
		
		for(int i=1;i<=c;i*=2)
		{
			System.out.println( i);
		}
